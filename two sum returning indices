class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {

        vector<int>ans(2);
        map<int,int>mpp;
        
        for(int i=0;i<nums.size();i++)
        {
            int extra = target - nums[i];

            if(mpp.find(extra) != mpp.end())
            {
                ans[0]=mpp[extra];
                ans[1]=i;
            }

            mpp[nums[i]]=i;


        }
        return ans;
    }
};
